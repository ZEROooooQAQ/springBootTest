<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.springboottest.demo.dao.UserMapper">

    <insert id="enroll" parameterType="com.springboottest.demo.domain.User">
        insert into user (name ,password ,email ,remark)
        values (#{name ,jdbcType=VARCHAR},#{password ,jdbcType=VARCHAR},#{email ,jdbcType=VARCHAR},#{remark ,jdbcType=VARCHAR})
    </insert>


</mapper>